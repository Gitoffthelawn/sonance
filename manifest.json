{

  "manifest_version": 2,
  "name": "sonance",
  "version": "1.0",
  "description": "Ambient sounds for your browser",

  "icons": {
    "48": "icons/sonance.png"
  },

  "browser_action": {
    "default_icon": "icons/sonance.png",
    "default_title": "Sonance",
    "default_popup": "options/index.html"
  },

  "web_accessible_resources": ["resources/*.mp3"]
}
